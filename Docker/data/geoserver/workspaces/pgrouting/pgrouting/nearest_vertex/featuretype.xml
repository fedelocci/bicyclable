FROM&#xd;
  ways_vertices_pgr AS v,&#xd;
  ways AS e&#xd;
WHERE&#xd;
  v.id = (SELECT&#xd;
            id&#xd;
          FROM ways_vertices_pgr&#xd;
          ORDER BY the_geom &lt;-&gt; ST_SetSRID(ST_MakePoint(%x%, %y%), 4326) LIMIT 1&#xd;
